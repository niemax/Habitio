{
  "expo": {
    "name": "Habitio",
    "slug": "Habitio",
    "userInterfaceStyle": "automatic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/brand.png",
    "splash": {
      "image": "./src/assets/brand.png",
      "resizeMode": "contain",
      "backgroundColor": "#F1F1F1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.niemax.Habitio",
      "buildNumber": "18"
    },
    "description": "",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./src/assets/brand.png",
          "mode": "production"
        }
      ]
    ],
    "android": {
      "package": "com.niemax.Habitio"
    }
  }
}
